{
  "name": "Bug Magnet日本語版",
  "description": "Bug Magnetの日本語版。探索的テストに役立つ右クリックのコンテキストメニュー",
  "version": "0.0.1",
  "permissions": [
    "scripting",
    "contextMenus",
    "storage",
    "activeTab"
  ],
  "optional_permissions": [
    "clipboardWrite",
    "clipboardRead"
  ],
  "background": {
    "service_worker": "extension.js"
  },
  "manifest_version": 3,
  "icons": {
    "128": "magnet.png",
    "16": "magnet-16.png"
  },
  "homepage_url": "https://github.com/rmuraoka/bugmagnet",
  "author": "Risa Muraoka",
  "options_ui": {
    "page": "options.html"
  }
}
