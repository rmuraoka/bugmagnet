{
  "name": "Bug Magnet日本語版",
  "description": "Bug Magnetの日本語版。探索的テストに役立つ右クリックのコンテキストメニュー",
  "version": "1.1.0",
  "default_locale": "en",
  "permissions": [
    "contextMenus",
    "storage",
    "activeTab"
  ],
  "optional_permissions": [
    "clipboardWrite",
    "clipboardRead"
  ],
  "background": {
    "scripts": [
      "extension.js"
    ]
  },
  "manifest_version": 2,
  "icons": {
    "128": "magnet.png",
    "16": "magnet-16.png"
  },
  "homepage_url": "https://github.com/rmuraoka/bugmagnet",
  "author": "Risa Muraoka",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
